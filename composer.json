{
    "name": "nevadskiy/laravel-position",
    "description": "Arrange Laravel models in a given order.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "position",
        "sort",
        "order",
        "arrange",
        "model",
        "eloquent",
        "laravel"
    ],
    "homepage": "https://github.com/nevadskiy/laravel-position",
    "authors": [
        {
            "name": "Nevadskiy",
            "email": "nevadskiy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.2",
        "laravel/framework": "^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0|^10.5",
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Nevadskiy\\Position\\": "src/"
